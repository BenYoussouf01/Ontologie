@prefix : <http://abdourahmane.com/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://abdourahmane.com/ontology/FactsDomainKnowledge> rdf:type owl:Ontology ;
    owl:imports <https://raw.githubusercontent.com/BenYoussouf01/Ontologie/main/IdentityUsersGroupsRoles.owl> .

############################
### CLASSES
############################

:Fact rdf:type owl:Class .

:SensitiveAction rdf:type owl:Class ;
    rdfs:subClassOf :Fact .

############################
### DATA PROPERTIES
############################

:sensitive rdf:type owl:DatatypeProperty ;
    rdfs:domain :Fact ;
    rdfs:range xsd:boolean .

############################
### OBJECT PROPERTIES
############################

:requiresVerificationForGroup rdf:type owl:ObjectProperty ;
    rdfs:domain :SensitiveAction ;
    rdfs:range :Group .
